<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UpgradeActivity">

    <ImageButton
        android:id="@+id/imageSpeech"
        android:layout_width="85dp"
        android:layout_height="100dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/smoel" />

    <TextView
        android:id="@+id/titleSpeech"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Motiverende speech"
        app:layout_constraintStart_toEndOf="@+id/imageSpeech"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/amountSpeech"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="0"
        app:layout_constraintStart_toEndOf="@+id/imageSpeech"
        app:layout_constraintTop_toBottomOf="@+id/titleSpeech" />

    <TextView
        android:id="@+id/outputSpeech"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="1 Jeffreys/s"
        app:layout_constraintStart_toEndOf="@+id/imageSpeech"
        app:layout_constraintTop_toBottomOf="@+id/amountSpeech" />

    <TextView
        android:id="@+id/costSpeech"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Prijs: 5"
        app:layout_constraintStart_toEndOf="@+id/imageSpeech"
        app:layout_constraintTop_toBottomOf="@+id/outputSpeech" />

    <ImageButton
        android:id="@+id/imageKantoor"
        android:layout_width="85dp"
        android:layout_height="100dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageSpeech"
        app:srcCompat="@drawable/giacentre" />

    <TextView
        android:id="@+id/titleKantoor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Nieuw kantoor"
        app:layout_constraintStart_toEndOf="@+id/imageKantoor"
        app:layout_constraintTop_toBottomOf="@+id/imageSpeech" />

    <TextView
        android:id="@+id/amountKantoor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="0"
        app:layout_constraintStart_toEndOf="@+id/imageKantoor"
        app:layout_constraintTop_toBottomOf="@+id/titleKantoor" />

    <TextView
        android:id="@+id/outputKantoor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="5 Jeffreys/s"
        app:layout_constraintStart_toEndOf="@+id/imageKantoor"
        app:layout_constraintTop_toBottomOf="@+id/amountKantoor" />

    <TextView
        android:id="@+id/costKantoor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Prijs: 100"
        app:layout_constraintStart_toEndOf="@+id/imageKantoor"
        app:layout_constraintTop_toBottomOf="@+id/outputKantoor" />

    <ImageButton
        android:id="@+id/imageWingchun"
        android:layout_width="85dp"
        android:layout_height="100dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageKantoor"
        app:srcCompat="@drawable/wingchun" />

    <TextView
        android:id="@+id/titleWingchun"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Wing Chunner"
        app:layout_constraintStart_toEndOf="@+id/imageWingchun"
        app:layout_constraintTop_toBottomOf="@+id/imageKantoor" />

    <TextView
        android:id="@+id/amountWingchun"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="0"
        app:layout_constraintStart_toEndOf="@+id/imageWingchun"
        app:layout_constraintTop_toBottomOf="@+id/titleWingchun" />

    <TextView
        android:id="@+id/outputWingchun"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="20 Jeffreys/s"
        app:layout_constraintStart_toEndOf="@+id/imageWingchun"
        app:layout_constraintTop_toBottomOf="@+id/amountWingchun" />

    <TextView
        android:id="@+id/costWingchun"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Prijs: 1000"
        app:layout_constraintStart_toEndOf="@+id/imageWingchun"
        app:layout_constraintTop_toBottomOf="@+id/outputWingchun" />

    <ImageButton
        android:id="@+id/imageDuitsland"
        android:layout_width="85dp"
        android:layout_height="100dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/duitsland"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageWingchun" />

    <TextView
        android:id="@+id/titleDuitsland"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Deutsche Upgrade"
        app:layout_constraintStart_toEndOf="@+id/imageDuitsland"
        app:layout_constraintTop_toBottomOf="@+id/imageWingchun" />

    <TextView
        android:id="@+id/amountDuitsland"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="0"
        app:layout_constraintStart_toEndOf="@+id/imageDuitsland"
        app:layout_constraintTop_toBottomOf="@+id/titleDuitsland" />

    <TextView
        android:id="@+id/outputDuitsland"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="88 Jeffreys/s"
        app:layout_constraintStart_toEndOf="@+id/imageDuitsland"
        app:layout_constraintTop_toBottomOf="@+id/amountDuitsland" />

    <TextView
        android:id="@+id/costDuitsland"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Prijs: 1500"
        app:layout_constraintStart_toEndOf="@+id/imageDuitsland"
        app:layout_constraintTop_toBottomOf="@+id/outputDuitsland" />

    <ImageButton
        android:id="@+id/imageAuto"
        android:layout_width="85dp"
        android:layout_height="100dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageDuitsland"
        app:srcCompat="@drawable/auto" />

    <TextView
        android:id="@+id/titleAuto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Nieuwe auto"
        app:layout_constraintStart_toEndOf="@+id/imageAuto"
        app:layout_constraintTop_toBottomOf="@+id/imageDuitsland" />

    <TextView
        android:id="@+id/amountAuto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="0"
        app:layout_constraintStart_toEndOf="@+id/imageAuto"
        app:layout_constraintTop_toBottomOf="@+id/titleAuto" />

    <TextView
        android:id="@+id/outputAuto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="500 Jeffreys/s"
        app:layout_constraintStart_toEndOf="@+id/imageAuto"
        app:layout_constraintTop_toBottomOf="@+id/amountAuto" />

    <TextView
        android:id="@+id/costAuto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Prijs: 10000"
        app:layout_constraintStart_toEndOf="@+id/imageAuto"
        app:layout_constraintTop_toBottomOf="@+id/outputAuto" />

    <ImageButton
        android:id="@+id/imageBaby"
        android:layout_width="85dp"
        android:layout_height="100dp"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageAuto"
        app:srcCompat="@drawable/baby" />

    <TextView
        android:id="@+id/titleBaby"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Nieuwe Baby"
        app:layout_constraintStart_toEndOf="@+id/imageBaby"
        app:layout_constraintTop_toBottomOf="@+id/imageAuto" />

    <TextView
        android:id="@+id/amountBaby"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="0"
        app:layout_constraintStart_toEndOf="@+id/imageBaby"
        app:layout_constraintTop_toBottomOf="@+id/titleBaby" />

    <TextView
        android:id="@+id/outputBaby"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="10000 Jeffreys/s"
        app:layout_constraintStart_toEndOf="@+id/imageBaby"
        app:layout_constraintTop_toBottomOf="@+id/amountBaby" />

    <TextView
        android:id="@+id/costBaby"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        android:text="Prijs: 1000000"
        app:layout_constraintStart_toEndOf="@+id/imageBaby"
        app:layout_constraintTop_toBottomOf="@+id/outputBaby" />

</android.support.constraint.ConstraintLayout>